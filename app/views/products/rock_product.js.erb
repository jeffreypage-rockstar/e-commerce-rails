$('#fav').html( "Favorited (<%=@product.product_rocks.size%>)");
<%if @flag == true%>
	$("#fav_login").html('<%=link_to image_tag("/assets/main/fav-icon-blue.png")+"Unfavorite",rock_product_path(@product,:unrock=>true),:remote=>true,:class=>"add-fav"%>');
<%else%>
	$("#fav_login").html('<%=link_to image_tag("/assets/main/fav-icon-blue.png")+" Add to favorites",rock_product_path(@product),:remote=>true,:class=>"add-fav"%>');
<%end%>