              <h1><span>Purchase History</span></h1>
            </div>
          </div>
                    <div class="row-fluid">
            
            <%@products.each_with_index do |product,index|%>
              
              <%=render partial: "products/product",locals: {product: product,:variant=> nil}%>
             
