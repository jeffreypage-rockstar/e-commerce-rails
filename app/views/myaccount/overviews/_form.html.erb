<div id='myaccount-profile_name_form' class='clearfix'>
  <fieldset id="myaccount-profile" class='five columns'>
     <ul>
       <li>
         <%= f.label :first_name %>
         <%= f.text_field :first_name, :placeholder => "First Name" %>
        </li>
        <li>
          <%= f.label :last_name %>
          <%= f.text_field :last_name %>
        </li>
        <li>
          <%= f.label :email %>
          <%= f.text_field :email %>
        </li>
        <li>
          <%= f.label :bday %>
          <%= f.text_field :bday %>
        </li>

        <li>
          <%= f.label :phone %>
          <%= f.text_field :phone %>
        </li>

        <li>
          <%= f.label :about_me %>
          <%= f.text_area :about_me ,:rows=> 3%>
        </li>
        <li>
          <%= f.fields_for :images, @image do |image_form|%>
            <label>Photo</label>
            <% if !image_form.object.new_record? %>
              <%= image_tag image_form.object.photo.url(:small) %>
            <% end %>
            <%= image_form.file_field   :photo %>
          <% end %>
        
        </li>
     </ul>
  </fieldset>
</div>
<div id='myaccount-profile_form' class='clearfix' >
  <fieldset id="myaccount-profile-password" class='five '>
    <ul>
      <li>
        <%= f.label :password %>
        <%= f.password_field :password %>
      </li>
      <li>
        <%= f.label :password_confirmation %>
        <%= f.password_field :password_confirmation %>
      </li>
    </ul>
  </fieldset>
  <div class="actions"><%= f.submit 'Update' , :class => 'button' %></div>
</div>
